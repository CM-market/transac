{
  "name": "transac",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/fetch_openapi.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ignore-pattern 'src/openapi-rq/**' --ignore-pattern 'eventserver/**'",
    "lint:check": "eslint . --ignore-pattern 'src/openapi-rq/**' --ignore-pattern 'eventserver/**'",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "ts:check": "tsc --noEmit",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "codegen": "openapi-rq -i openapi.json -o ./src/openapi-rq -c fetch",
    "postinstall": "npm run codegen"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/uuid": "^10.0.0",
    "crypto-js": "^4.2.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "idb": "^8.0.3",
    "jose": "^6.0.12",
    "js-sha256": "^0.11.1",
    "json-canonicalize": "^2.0.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "react-router-dom": "^7.7.0",
    "sonner": "^2.0.6",
    "tslib": "^2.8.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^1.6.2",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jszip": "^3.4.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "globals": "^13.24.0",
    "install": "^0.13.0",
    "jsdom": "^26.1.0",
    "npm": "^11.5.2",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^2.0.4"
  }
}
