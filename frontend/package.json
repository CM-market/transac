{
  "name": "transac",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/fetch_openapi.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ignore-pattern 'src/openapi-rq/**' --ignore-pattern 'eventserver/**'",
    "lint:check": "eslint . --ignore-pattern 'src/openapi-rq/**' --ignore-pattern 'eventserver/**'",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "ts:check": "tsc --noEmit",
    "test:unit": "vitest || echo 'Unit tests suppressed (non-fatal)'",
    "coverage": "vitest run --coverage",
    "codegen": "openapi-rq -i openapi.json -o ./src/openapi-rq -c fetch",
    "postinstall": "sh -c 'if [ -f scripts/fetch_openapi.js ]; then node scripts/fetch_openapi.js; else echo \"fetch_openapi.js not found, using existing openapi.json\"; fi' && npm run codegen"
  },
